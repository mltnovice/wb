.fade-wrapper
  transform translateY(100px)
.fadeIn
  transform translateY(0)
  transition all .5s ease-out
  &:nth-child(1)
    transition all .5s ease-out 0.1s
  &:nth-child(2)
    transition all .5s ease-out 0.2s
  &:nth-child(3)
    transition all .5s ease-out 0.3s
  &:nth-child(4)
    transition all .5s ease-out 0.4s
